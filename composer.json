{
    "name": "cristianhr/extended-bread-form-fields",
    "description": "Extended FormFields hook interface to Voyager BREAD",
    "license": "MIT",
    "authors": [
        {
            "name": "CristianHR",
            "email": "khristiam_r@hotmail.com"
        }
    ],
    "require": {
        "tcg/voyager": "1.*"
    },
    "autoload": {
        "psr-4": {
            "ExtendedBreadFormFields\\": "src/"
        }
